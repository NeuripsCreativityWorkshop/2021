{
  "name": "readerwebapp",
  "version": "1.0.0",
  "description": "Machine Learning for Creativity and Design Website",
  "main": "index.js",
  "repository": "https://github.com/NeuripsCreativityWorkshop/2021.git",
  "author": "Lia Coleman <liailiad@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:dev": "yarn run clean-dist && webpack --config=configs/webpack/dev.js",
    "build:prod": "yarn run clean-dist && webpack --config=configs/webpack/prod.js",
    "create:pub": "webpack --config=configs/webpack/publish.js",
    "webversion": "webpack -v",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint ./src/**/*.{js,ts,tsx} --quiet",
    "lint:fix": "eslint ./src/**/*.{js,ts,tsx} --fix",
    "lint-staged": "^12.3.2",
    "start": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "webpack serve --config=configs/webpack/prod.js",
    "test": "jest --coverage --watchAll=false --config=configs/test/jest.config.js",
    "test:coverage": "jest --passWithNoTests --coverage --watchAll=false --config=configs/test/jest.config.js",
    "lint-commit": "lint-staged",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-loader": "^8.2.3",
    "create-file-webpack": "^1.0.2",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.5.1",
    "dotenv": "^15.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^27.4.7",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.3.3",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "querystring-es3": "^0.2.1",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.4",
    "sass-loader": "^12.4.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.5.5",
    "url": "^0.11.0",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.0",
    "gapi-script": "^1.2.0",
    "react": "^16.9.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^16.9.0",
    "react-material-ui-carousel": "^3.3.3",
    "react-router-dom": "5"
  },
  "lint-staged": {
    "*.{tsx,ts,js,scss,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
